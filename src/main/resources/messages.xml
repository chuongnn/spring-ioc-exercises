<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="http://www.springframework.org/schema/beans
        http://www.springframework.org/schema/beans/spring-beans.xsd">

    <bean id="frenchMessage" class="home.spring.ioc.exercises.first.MessageFactory"
          factory-method="createFrenchMessage"/>

    <bean id="englishMessage" class="home.spring.ioc.exercises.first.MessageFactory"
          factory-method="createEnglishMessage"/>

    <alias name="frenchMessage" alias="publishedMessage"/>
</beans>
